public class FourSquare {
    public static void main(String[] args) {
        SimpleRobot robot = new SimpleRobot();
	for(int i = 0;i < 4;i++){
	    robot.moveForward(100);
	    robot.turnright(45);
	    robot.moveForward(100);
	    robot.turnleft(90);
	    robot.moveForward(100);
	    robot.turnleft(90);
	    robot.moveForward(100);
	    robot.turnright(120);
	    robot.moveForward(100);
	     robot.turnleft(90);
        }
    }
    }
}
